{
  "name": "idbook",
  "displayName": "idbook",
  "description": "A VS Code extenstion to store, manage, and reference GUIDs",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tuckers15G/idbook.git"
  },
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "idbook.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "idbook.createOrLocateConfig",
        "title": "Create ID Book"
      },
      {
        "command": "idbook.insertReference",
        "title": "Insert Reference (ID Book)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
